#include<bits/stdc++.h>
using namespace std;

int solution(int arr[],int n)
{
    int ans = 0;
        for(int i = 0 ; i < n; i++){
            ans ^= (2 * arr[i]);
}
return ans;
}

int main()
{
    int n;cin>>n;
    int arr[n];
    for(int i = 0 ; i < n; i++){
            cin>>arr[i];
    }
    cout<<solution(arr,n);
}
